pipeline {
    agent any
    stages {

        stage('Build da imagem Docker Frontend') {
            steps {
                script {
                 sh 'docker buildx build -t frontend -f Dockerfile.frontend .'
            
}
}
        }
         stage('Build da imagem Docker backend') {
            steps {
                script {
                 sh 'docker buildx build -t backend -f Dockerfile.backend .'
            
}
}
        }



    }
}
